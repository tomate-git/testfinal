{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://www.google.com https://maps.google.com;"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://maison-less.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ],
  "rewrites": [ 
    { "source": "/:path*", "destination": "/index.html" } 
  ]
}